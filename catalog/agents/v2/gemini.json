{
  "id": "agent:gemini",
  "kind": "agent",
  "name": "Gemini CLI",
  "version": "latest",

  "delivery": "remote",
  "install": {
    "source": "npm",
    "package": "@google/gemini-cli"
  },

  "bins": ["gemini"],

  "detect": {
    "command": "gemini --version",
    "expectExitCode": 0
  },

  "auth": {
    "required": true,
    "command": "gemini",
    "instructions": "Run 'gemini' to authenticate with your Google account"
  },

  "meta": {
    "description": "Google Gemini CLI - AI coding assistant",
    "category": "ai-assistant",
    "homepage": "https://gemini.google.com"
  }
}
