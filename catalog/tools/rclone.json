{
  "id": "tool:rclone",
  "name": "rclone",
  "version": "1.68.2",
  "description": "Cloud storage CLI (Google Drive, S3, Dropbox, 40+ providers)",
  "category": "utilities",
  "installType": "binary",
  "binary": "rclone",
  "upstream": {
    "darwin-arm64": "https://downloads.rclone.org/v1.68.2/rclone-v1.68.2-osx-arm64.zip",
    "darwin-x64": "https://downloads.rclone.org/v1.68.2/rclone-v1.68.2-osx-amd64.zip",
    "linux-x64": "https://downloads.rclone.org/v1.68.2/rclone-v1.68.2-linux-amd64.zip"
  },
  "extract": {
    "default": { "type": "zip", "binary": "rclone-*/rclone" }
  },
  "requiresAuth": true,
  "authCommand": "rclone config"
}
