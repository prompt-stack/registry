{
  "id": "stack:data-analysis",
  "kind": "stack",
  "name": "Data Analysis",
  "version": "1.0.0",

  "delivery": "remote",
  "install": {
    "source": "catalog",
    "path": "catalog/stacks/data-analysis"
  },

  "runtime": "python",
  "requires": {
    "binaries": [],
    "secrets": []
  },
  "provides": {
    "tools": [
      "data_load_csv",
      "data_load_excel",
      "data_load_json",
      "data_describe",
      "data_query",
      "data_transform",
      "data_aggregate",
      "data_chart",
      "data_export",
      "data_list"
    ]
  },
  "mcp": {
    "transport": "stdio",
    "command": "python",
    "args": ["src/server.py"],
    "cwd": "."
  },

  "meta": {
    "description": "Analyze data with Python, pandas, numpy, and matplotlib. Generate charts and insights.",
    "author": "RUDI",
    "license": "MIT",
    "category": "data",
    "tags": ["data", "analysis", "python", "pandas", "charts"],
    "icon": "ðŸ“ˆ"
  }
}
