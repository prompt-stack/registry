{
  "id": "ms-office",
  "name": "ms-office",
  "version": "1.0.0",
  "description": "MCP server for reading and editing Microsoft Office documents (Word, Excel)",
  "runtime": "python",
  "command": [
    "python",
    "server.py"
  ],
  "provides": {
    "tools": [
      "docx_read",
      "docx_replace_text",
      "docx_edit_table",
      "docx_update_table_column",
      "docx_delete_table_column",
      "docx_list_tables",
      "docx_insert_paragraphs",
      "docx_create",
      "xlsx_read",
      "xlsx_list_sheets",
      "xlsx_write_cell",
      "xlsx_write_row",
      "xlsx_append_row",
      "xlsx_create"
    ]
  },
  "requires": {
    "binaries": [],
    "secrets": []
  }
}
