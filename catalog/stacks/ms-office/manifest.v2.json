{
  "id": "stack:ms-office",
  "kind": "stack",
  "name": "MS Office",
  "version": "1.0.0",

  "delivery": "remote",
  "install": {
    "source": "catalog",
    "path": "catalog/stacks/ms-office"
  },

  "runtime": "node",
  "requires": {
    "binaries": [],
    "secrets": []
  },
  "provides": {
    "tools": ["docx_read", "xlsx_read"]
  },
  "mcp": {
    "transport": "stdio",
    "command": "npx",
    "args": ["tsx", "src/index.ts"],
    "cwd": "."
  },

  "meta": {
    "description": "Read and extract content from Microsoft Word (.docx) and Excel (.xlsx) documents",
    "author": "CO-LLAB",
    "license": "MIT",
    "category": "document-processing",
    "tags": ["microsoft", "office", "word", "excel", "docx", "xlsx"],
    "icon": "ðŸ“„"
  }
}
