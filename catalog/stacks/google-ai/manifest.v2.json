{
  "id": "stack:google-ai",
  "kind": "stack",
  "name": "Google AI Suite",
  "version": "1.0.0",

  "delivery": "remote",
  "install": {
    "source": "catalog",
    "path": "catalog/stacks/google-ai"
  },

  "runtime": "node",
  "requires": {
    "binaries": [],
    "secrets": [
      {
        "key": "GOOGLE_AI_API_KEY",
        "label": "Google AI API Key",
        "required": true,
        "helpUrl": "https://makersuite.google.com/app/apikey"
      }
    ]
  },
  "provides": {
    "tools": ["generate_image", "generate_video"]
  },
  "mcp": {
    "transport": "stdio",
    "command": "npx",
    "args": ["tsx", "src/index.ts"],
    "cwd": "."
  },

  "meta": {
    "description": "Generate images (Gemini, Imagen 4) and videos (Veo 3.1) using Google AI",
    "author": "CO-LLAB",
    "license": "MIT",
    "category": "ai-generation",
    "tags": ["ai", "google-ai", "gemini", "imagen", "veo"],
    "icon": "ðŸŽ¨"
  }
}
