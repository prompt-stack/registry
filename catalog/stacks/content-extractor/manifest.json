{
  "id": "content-extractor",
  "name": "Content Extractor",
  "version": "1.0.0",
  "description": "Extract content from YouTube, Reddit, TikTok, and web articles",
  "author": "CO-LLAB",
  "license": "MIT",
  "runtimes": ["node"],
  "mcp": {
    "runtime": "node",
    "entry": "node/src/index.ts",
    "command": "npx",
    "args": ["tsx", "node/src/index.ts"]
  },
  "secrets": [
    {
      "key": "SUPA_DATA_API",
      "label": "Supadata API Key",
      "description": "Optional - Supadata API for faster YouTube transcript extraction",
      "helpUrl": "https://supadata.ai",
      "required": false
    }
  ],
  "tools": [
    "extract_youtube",
    "extract_reddit",
    "extract_tiktok",
    "extract_article"
  ],
  "tags": ["youtube", "reddit", "tiktok", "article", "transcript", "extraction", "scraping"],
  "category": "data-extraction",
