    "sheets_read",
    "sheets_write",
    "sheets_append",
    "sheets_create",
    "docs_create",
    "docs_read",
    "docs_insert_image",
    "drive_list",
    "drive_upload",
    "drive_make_public",
    "drive_delete",
    "calendar_list",
    "calendar_create",
    "calendar_quick_add",
    "calendar_delete"
  ],
