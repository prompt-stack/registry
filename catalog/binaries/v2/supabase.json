{
  "id": "binary:supabase",
  "kind": "binary",
  "name": "Supabase CLI",
  "version": "latest",

  "delivery": "remote",
  "install": {
    "source": "npm",
    "package": "supabase"
  },

  "bins": ["supabase"],

  "detect": {
    "command": "supabase --version",
    "expectExitCode": 0
  },

  "auth": {
    "required": false,
    "command": "supabase login",
    "instructions": "Run 'supabase login' to authenticate (optional for local development)"
  },

  "meta": {
    "description": "Supabase local development and deployment",
    "category": "devops"
  }
}
